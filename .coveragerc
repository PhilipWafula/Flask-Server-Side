[run]
source=.
omit=
    # omit tests folder
    */tests/*
    # omit devtools folder
    /devtools/*
    # omit postman collections
    /collections/*
    # omit migrations
    /app/migrations/*
    # omit run_test_suite_file
    /run_test_suite.py
    # omit config parser
    /app/config.py
    # omit run.py TODO: [Philip]: Find out if there are ways to test the run file
    /app/run.py
    # omit manage file
    /app/manage.py